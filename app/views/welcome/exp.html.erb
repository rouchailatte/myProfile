<script type="text/javascript">
  
  $.fn.slideFadeToggle = function(){
    return this.animate({ opacity: 'toggle', height: 'toggle' }, 350, 'swing');
  };

  var rotateTwo = false;
  $('#buttonTwo').on('click', function(){
    $('.responderTwo').slideFadeToggle();
    if(!rotateTwo)
    {
      $('#buttonTwo').addClass('rotate');
      rotateTwo = true;
    }
    else
    {
      $('#buttonTwo').removeClass('rotate');
      rotateTwo = false;
    }
  });
</script>